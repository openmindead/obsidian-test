{
  "database": "Zotero",
  "noteImportFolder": "!Notes/Fleeting notes/",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "png",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "pandoc",
      "brackets": true
    }
  ],
  "exportFormats": [
    {
      "name": "Make fleeting note",
      "outputPathTemplate": "!Notes/Fleeting notes/fleeting_{{citekey}}.md",
      "imageOutputPathTemplate": "Files/{{citekey}}/",
      "imageBaseNameTemplate": "image"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "last-imported-note",
  "shouldConcat": true
}